{
  "java": {
    "compile": "javac Main.java",
    "invoke": "java Main",
    "file_name": "Main.java"
  },
  "python": {
    "compile": "",
    "invoke": "python3 app.py",
    "file_name": "app.py"
  },
  "csharp": {
    "compile": "mcs -out:app.exe app.cs",
    "invoke": "python3 app.py",
    "file_name": "mono app.exe"
  },
  "js": {
    "compile": "",
    "invoke": "node app.js",
    "file_name": "app.js"
  },
  "ts": {
    "compile": "tsc app.ts",
    "invoke": "node app.js",
    "file_name": "app.ts"
  },
  "kotlin": {
    "compile": "kotlinc app.kt -include-runtime -d app.jar",
    "invoke": "java -jar app.jar",
    "file_name": "app.kt"
  }
}
